{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "insertPragma": true,
  "proseWrap": "never",
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto"
}
